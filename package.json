{
	"name": "control-snippets",
	"displayName": "Control Snippets",
	"description": "Disable or enable VS Code's built-in snippets and manually installed snippets from extensions.",
	"version": "1.9.1",
	"publisher": "svipas",
	"main": "./dist/main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/svipas/vscode-control-snippets.git"
	},
	"bugs": {
		"url": "https://github.com/svipas/vscode-control-snippets/issues"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.30.0"
	},
	"extensionKind": [
		"workspace"
	],
	"categories": [
		"Other",
		"Snippets",
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.control-snippets",
				"title": "Control Snippets"
			}
		]
	},
	"scripts": {
		"build": "webpack --mode production",
		"pretest": "tsc -p ./ && yarn build",
		"test": "node ./out/test/runTest.js",
		"vscode:publish": "vsce publish --yarn",
		"vscode:prepublish": "yarn build"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.0.18",
		"@types/sinon": "^9.0.4",
		"@types/vscode": "^1.30.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"sinon": "^9.0.2",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.6",
		"vscode-test": "^1.4.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"keywords": [
		"override",
		"enable",
		"disable",
		"global",
		"local",
		"installed",
