{
  "name": "control-snippets",
  "displayName": "Control Snippets",
  "description": "Disable or enable VS Code's built-in snippets and manually installed snippets from extensions.",
  "version": "1.3.0",
  "publisher": "svipas",
  "repository": {
    "type": "git",
    "url": "https://github.com/svipas/vscode-control-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/svipas/vscode-control-snippets/issues"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:extension.control-snippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.control-snippets",
        "title": "Control Snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile",
    "test": "node ./out/test/run-test.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.17",
    "@types/vscode": "^1.30.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "typescript": "^3.6.2",
    "vscode-test": "^1.2.0"
  },
  "keywords": [
    "enable",
    "disable",
    "global",
    "builtin",
    "built-in",
    "code",
    "control",
    "snippet",
    "snippets",
    "extension",
    "extensions",
    "javascript",
    "js",
    "jsx",
    "flow",
    "typescript",
    "ts",
    "json",
    "html",
    "vue",
    "angular",
    "css",
    "less",
    "scss",
    "styled-components",
    "styled-jsx",
    "graphql",
    "markdown",
    "md",
    "commonmark",
    "mdx",
    "yaml",
    "yml",
    "elm",
    "java",
    "php",
    "postgresql",
    "sql",
    "ruby",
    "swift",
    "toml"
  ]
}
